journal = {Experimental brain research},
keywords = {Adolescent,Adult,Algorithms,Computer Simulation,Cross-Over Studies,Data Interpretation, Statistical,Executive Function,Executive Function: physiology,Feedback, Psychological,Female,Humans,Learning,Learning: physiology,Male,Motor Skills,Motor Skills: physiology,Psychomotor Performance,Psychomotor Performance: physiology,Robotics,Time Perception,Time Perception: physiology,Video Games,Wrist,Wrist: physiology,Young Adult},
month = {mar},
number = {2},
pages = {119--31},
pmid = {19787345},
title = {{Comparison of error-amplification and haptic-guidance training techniques for learning of a timing-based motor task by healthy individuals.}},
