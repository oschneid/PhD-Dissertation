keywords = {Actuators,Aerospace electronics,Haptic interfaces,Prototypes,Rhythm,Software,VT customization tools,VT design control,Vibrations,affective tactile messages,consumer device,consumer electronics,control granularity,design space,end user customization,haptic interfaces,qualitative tool parameter examination,ubiquitous computing,vibrations,vibrotactile signals},
month = feb,
pages = {251--256},
publisher = {IEEE},
